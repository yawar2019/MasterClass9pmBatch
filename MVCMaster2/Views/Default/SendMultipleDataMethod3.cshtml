@model MasterClass9pmBatch.Models.EmpDept
@{
    ViewBag.Title = "SendMultipleDataMethod3";
}

<h2>SendMultipleDataMethod3</h2>

<table class="table table-bordered table-condensed table-responsive">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>

    @foreach (var item in Model.emp)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>

<table class="table table-bordered table-condensed table-responsive">
    <tr>
        <th>DeptId</th>
        <th>DeptName</th>
    </tr>

    @foreach (var item in Model.dept)
    {
        <tr>
            <td>@item.DeptId</td>
            <td>@item.DeptName</td>
        </tr>
    }
</table>